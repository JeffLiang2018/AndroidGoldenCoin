            android:name=".Setting"
            android:label="Setting">
            <intent-filter>
                <action android:name="com.example.jeffliang.coen268finalproject.Setting" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".SecuritySetting" />
        <activity android:name=".RequireAuthentication" />
        <activity android:name=".NotificationList" />
        <activity android:name=".History" />
